{% extends ':base:base.html.twig' %}

{% block title %}Cache{% endblock title %}

{% block body %}
<div id="page-body">
<div id="main">
	<h2 class="imgrep">Cache</h2>
	{% if authorised %}
	<p>You just completed an action to to {{ cache }} the cache.</p><br /><br /><br />
	{% else %}
	<p>You don't have acces to {{ cache }} the cache</p>
	{% endif %}
</div>

<div id="extras">
	<div class="mini-panel">
		<div class="inner"><span class="corners-top"><span></span></span>
		<h3>Quick Links</h3>
		<ul class="menu">
			<li><a href="/tools/cache/clear/{{ password }}/">Cache Clear</a></li>
			<li class="last"><a href="/tools/cache/warmup/{{ password }}/">Cache Warmup</a></li>
		</ul>
		<span class="corners-bottom"><span></span></span></div>
	</div>
</div>

</div>
{% endblock %}
